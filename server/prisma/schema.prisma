generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tweet {
  id         String    @default(dbgenerated()) @id
  created    DateTime? @default(now())
  message    String
  tweet_user String
  User       User      @relation(fields: [tweet_user], references: [id])
}

model User {
  id        String  @default(dbgenerated()) @id
  name      String
  email     String  @unique
  user_name String  @unique
  pw        String
  Tweet     Tweet[]
}
